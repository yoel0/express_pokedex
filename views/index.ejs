<div class="container">
  <h1 class="text-center">The Original 151 Pokemon</h1>
  <% pokemon.forEach((pokemon) => { %>
  <div class="well">
    <h2><%= pokemon.name %></h2>
    <form method="POST" action="/pokemon">
      <input hidden type="text" name="name" value="<%= pokemon.name %>" />
      <button class="btn waves-effect red darken-4" type="submit">
        Add to Favorite Pokemon <i class="material-icons right">send</i>
      </button>
    </form>
  </div>
  <% }); %>
</div>
