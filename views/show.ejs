<p class="flow-text"><%= pokedata.order %> - <%= pokedata.name %></p>

<div class="row">
  <div class="col s4">
    Normal Version
    <hr />
    <img
      class="sprite"
      src="<%= pokedata.sprites.front_default %>"
      alt="Sprite"
    />
  </div>
  <div class="col s4">
    Shiny Version
    <hr />
    <img
      class="sprite"
      src="<%= pokedata.sprites.front_shiny %>"
      alt="Sprite Shiny"
    />
  </div>
  <div class="col s4" id="transwindow">
    height:<%= pokedata.height %> weight:<%= pokedata.weight %>
    <hr />
    Types: <% pokedata.types.forEach((t)=>{ %>
    <p class="<%= t.type.name %>"><%= t.type.name %></p>
    <% }); %>
  </div>
</div>
